{
	"protocol": "h2",
	"server_name": "cloudflare-dns.com",
	"disable_domain_sni": false,
	"disable_certificate_validation": true,
	"socket_addrs": "1.1.1.1:443",
	"udp_socket_addrs": "127.0.0.1:53",
	"custom_http_path": null,
	"fragmenting": {
		"enable": true,
		"method": "single",
		"sleep_interval_min": 10,
		"sleep_interval_max": 100
	},
	"noise": {
		"enable": true,
		"ntype": "dns",
		"content": "google.com",
		"packet_length": 1,
		"packets": 1,
		"sleep": 1000,
		"continues": false
	},
	"ipv6": {
		"enable": false,
		"protocol": "h2",
		"server_name": "cloudflare-dns.com",
		"disable_domain_sni": false,
		"disable_certificate_validation": true,
		"socket_addrs": "[2001:4860:4860::8888]:443",
		"udp_socket_addrs": "[::1]:53",
		"custom_http_path": null,
		"fragmenting": {
			"enable": false,
			"method": "single",
			"sleep_interval_min": 10,
			"sleep_interval_max": 100
		},
		"noise": {
			"enable": false,
			"ntype": "rand",
			"content": "google.com",
			"packet_length": 1,
			"packets": 3,
			"sleep": 500,
			"continues": false
		}
	},
	"quic": {
		"congestion_controller": "bbr",
		"keep_alive_interval": 5,
		"datagram_receive_buffer_size": 16384,
		"datagram_send_buffer_size": 8192,
		"connecting_timeout_sec": 2
	},
	"connection": {
		"h1_multi_connections": 3,
		"dot_nonblocking_dns_query_lifetime": 5,
		"reconnect_sleep": 1,
		"max_reconnect": 5,
		"max_reconnect_sleep": 30
	},
	"doh_server": {
		"enable": false,
		"alpn": ["h2", "http/1.1"],
		"listen_address": "127.0.0.1:443",
		"certificate": "cert.crt",
		"key": "key.pem",
		"log_errors": false
	},
	"rules": [
		{
			"options": [
				"googlesyndication",
				"analytic",
				"googletagmanager"
			],
			"target": {
				"block": null
			}
		}
	]
}