{
	"protocol": "h2",
	"server_name": "cloudflare-dns.com",
	"ip_as_sni": false,
	"disable_certificate_validation": false,
	"remote_addrs": "1.1.1.1:443",
	"interface": null,
	"serve_addrs": "[::1]:53",
	"custom_http_path": null,
	"http_method": "GET",
	"response_timeout": 5,
	"http_keep_alive": null,
	"fragmenting": {
		"enable": false,
		"method": "single",
		"sleep_interval": "10-100",
		"fragment_size": "50-100",
		"segments": 2
	},
	"noise": {
		"enable": false,
		"ntype": "tftp",
		"content": "google.com",
		"packet_length": "1-100",
		"packets": 8,
		"sleep": 50
	},
	"quic": {
		"congestion_controller": "bbr",
		"keep_alive_interval": 10,
		"connecting_timeout_sec": 2,
		"datagram_receive_buffer_size": null,
		"datagram_send_buffer_size": null,
		"packet_threshold": 12,
		"max_idle_timeout": 3600
	},
	"connection": {
		"h1_multi_connections": 3,
		"reconnect_sleep": 1
	},
	"tcp_socket_options": {
		"send_buffer_size": null,
		"recv_buffer_size": null,
		"nodelay": null,
		"keepalive": null,
		"linux": {
			"bind_to_device": null,
			"mss": null,
			"congestion": null
		}
	},
	"doh_server": {
		"enable": false,
		"alpn": ["h2", "http/1.1"],
		"listen_address": "127.0.0.1:443",
		"certificate": "cert.crt",
		"key": "key.pem",
		"cache_control": "max-age=3600",
		"response_timeout": [2, 3],
		"log_errors": false
	},
	"rules": [
		{
			"options": [
				"doubleclick.net",
				"google-analytics.com",
				"googlesyndication.com",
				"firebaseinstallations.googleapis.com",
				"www.googletagservices.com"
			],
			"target": {
				"block": null
			}
		}
	],
	"overwrite": null
}
